#include <stdio.h>
#include <stdbool.h>

int main(void)
{
	int arr[3][5] = { 0 };
	for (int i = 0; i < 3; i++)
	{
		for (int j = 0; j < 5; j++)
		{
			arr[i][j] = i * 5 + j;
		}
	}

	for (int i = 0; i < 3; i++)
	{
		for (int j = 0; j < 5; j++)
		{
			printf("%3d ", arr[i][j]);
		}
		printf("\n");
	}
}
